# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /house/details/mustache/:name         controllers.GoTController.viewMustache(name: String)
GET     /house/details/twirl/:name             controllers.GoTController.viewTwirl(name: String)

GET     /house/find                      controllers.GoT1Controller.view
POST    /house/find                      controllers.GoT1Controller.submit


# Map static resources from the /public folder to the /assets URL path
GET       /assets/*file         controllers.Assets.at(path="/public", file)

